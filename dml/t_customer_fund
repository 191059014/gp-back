# 清除账户资金信息
UPDATE test.t_customer_fund
SET accountTotalMoney = 0,
 freezeMoney = 0,
 usableMoney = 0,
 totalRechargeMoney = 0,
 totalWithdrawMoney = 0,
 totalProfitAndLossMoney = 0,
 totalMessageServiceMoney = 0,
 totalInAndOutMoney = 0,
 tradeFreezeMoney = 0,
 totalStrategyMoney = 0,
 totalStrategyOwnMoney = 0
WHERE
	userId = 'U1';